{
  "name": "koi-cap",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "dist/electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vueuse/core": "^12.5.0",
    "bytenode": "^1.5.6",
    "element-plus": "^2.9.4",
    "fabric": "^6.5.4",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "electron": "^34.1.1",
    "electron-builder": "^25.1.8",
    "typescript": "~5.7.2",
    "unplugin-auto-import": "^19.0.0",
    "unplugin-vue-components": "^28.0.0",
    "vite": "^6.1.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vite-plugin-optimizer": "^1.4.3",
    "vue-tsc": "^2.2.0"
  },
  "build": {
    "asar": false,
    "productName": "koiBox",
    "icon": "app.png",
    "directories": {
      "output": "build-electron"
    },
    "extraResources": [
      {
        "from": "plugins",
        "to": "../plugins"
      },
      {
        "from": "libvlc.dll",
        "to": "../libvlc.dll"
      },
      {
        "from": "libvlccore.dll",
        "to": "../libvlccore.dll"
      }
    ],
    "files": [
      "!node_modules/**/*",
      "!**/*/{*.ts,*.rs,*.vue}",
      "node_modules/bytenode",
      "node_modules/node-screenshots",
      "node_modules/node-screenshots-win32-x64-msvc",
      "node_modules/@nestjs/common",
      "node_modules/@nestjs/core",
      "node_modules/@nestjs/platform-express",
      "node_modules/reflect-metadata",
      "node_modules/rxjs",
      "node_modules/sqlite3",
      "koi-player/{index.js,koi-player.win32-x64-msvc.node,package.json}",
      "dist",
      "koi-server/dist"
    ]
  }
}
